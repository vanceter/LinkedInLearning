# fortune cookie app

import random
lucky_number = random.randint(1,100)

message = ["You will have a great day!", 
           "Today is going to suck!", 
           "Pretty sure you die today...", 
           "Today was a good day."]

print(f"{random.choice(message)} Your lucky number is {random.randint(1,100)}")


